    "react": "^16.9.0",
    "react-native": "^0.63.3",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-ui-lib": "^5.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.20",
    "prettier": "^1.19.1"
  }
}
