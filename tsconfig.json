{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "lib": ["es2015"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
